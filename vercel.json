{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "DEBUG": "false",
    "LOG_LEVEL": "INFO"
  },
  "functions": {
    "main.py": {
      "maxDuration": 300
    }
  }
}
