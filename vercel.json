{
  "version": 2,
  "builds": [
    {
      "src": "main_vercel.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main_vercel.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "WHISPER_MODEL_NAME": "tiny"
  },
  "functions": {
    "main_vercel.py": {
      "maxDuration": 300,
      "memory": 3008
    }
  }
}